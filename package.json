{
  "name": "apolloserver-graphql-typescript-backend",
  "private": true,
  "version": "1.0.0",
  "description": "Graphql Prisma ApolloServer Fastify TypeScript",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "ts-node src/index.ts",
    "start:dev": "nodemon -w src -e ts,json -x ts-node -r tsconfig-paths/register ./src/index.ts",
    "start:debug": "nodemon -w src -e ts,json -x ts-node -r tsconfig-paths/register ./src/index.ts --debug --watch",
    "migrate:dev": "yarn prisma migrate dev",
    "migrate:dev:create": "yarn prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "yarn prisma migrate reset --preview-feature",
    "migrate:deploy": "yarn prisma migrate deploy --preview-feature",
    "migrate:status": "yarn prisma migrate status --preview-feature",
    "migrate:resolve": "yarn prisma migrate resolve --preview-feature",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:seed": "prisma db seed",
    "db:seed:prod": "prisma db seed:prod",
    "test": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "yarn run migrate:up && yarn run prisma:generate && yarn run seed",
    "lint:check": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,json}'",
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "seeds:prod": "ts-node ./build/src/seeds.js",
    "push": "prisma db push --schema=/tmp/schema.prisma"
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@as-integrations/fastify": "^2.0.0",
    "@aws-sdk/client-s3": "^3.391.0",
    "@aws-sdk/client-sns": "^3.398.0",
    "@aws-sdk/lib-storage": "^3.391.0",
    "@fastify/compress": "^6.4.0",
    "@fastify/cors": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/mysql": "^4.1.0",
    "@fastify/rate-limit": "^8.0.1",
    "@paljs/plugins": "^5.3.3",
    "@prisma/client": "4.16.2",
    "amqplib": "^0.10.3",
    "aws-sdk": "^2.1438.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "connect-redis": "^7.1.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^3.2.0",
    "ejs": "^3.1.9",
    "express-rate-limit": "^6.7.0",
    "express-session": "^1.17.3",
    "fastify": "^4.18.0",
    "fastify-plugin": "^4.5.0",
    "formidable": "^3.5.0",
    "graphql": "^16.7.0",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.22.2",
    "graphql-upload": "^16.0.2",
    "handlebars": "^4.7.7",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.3",
    "nodemon": "^3.0.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.14.2",
    "pino-pretty": "^10.2.0",
    "qrcode": "^1.5.3",
    "redis": "^4.6.8",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "stream": "^0.0.2",
    "type-graphql": "2.0.0-beta.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@mermaid-js/mermaid-cli": "^10.5.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.64",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/formidable": "^3.4.1",
    "@types/graphql-fields": "^1.3.5",
    "@types/graphql-upload": "^16.0.0",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "compression": "^1.7.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "prisma": "^5.3.1",
    "prisma-dbml-generator": "^0.10.0",
    "prisma-erd-generator": "^1.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "typegraphql-prisma": "^0.27.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
